<a id="features-section"></a>
{{#with features-section}}
  <section class="section">
    <div class="container">
      <div class="row">
        <div class="col-12">
          <h2 class="section__title section__title--small-margin">
            {{title}}
          </h2>
        </div>
        <picture class="col-12 col-lg-7 mb-5">
          <source
            class="img-fluid"
            srcset="assets/images/Cable-cadena-2@2x.jpg"
            media="(min-width: 768px)"
          />
          <img
            class="img-fluid"
            src="assets/images/Cable-cadena-2.jpg"
            alt="{{image-alt}}"
          />
        </picture>
        <div class="col-12 col-lg-5 mb-5">
          <p>{{p1}}</p>
          <p>{{p2}}</p>
        </div>
      </div>
    </div>
  </section>
{{/with}}